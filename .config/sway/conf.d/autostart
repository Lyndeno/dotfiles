# Start geoclue
exec /usr/lib/geoclue-2.0/demos/agent

# Autostart desktop programs
exec steam -silent 
exec discord --start-minimized --disable-frame-rate-limit
exec element-desktop --hidden --disable-frame-rate-limit

# start notification daemon
exec mako

# start polkit agent
exec /usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1

# start gammastep
exec gammastep-indicator
